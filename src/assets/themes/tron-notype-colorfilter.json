{
    "colors": {
        "r": 170,
        "g": 207,
        "b": 209,
        "black": "#323440",
        "light_black": "#05080d",
        "grey": "#262828"
    },
    "cssvars": {
        "font_main": "United Sans Medium",
        "font_main_light": "United Sans Light"
    },
    "terminal": {
        "fontFamily": "Fira Mono",
        "cursorStyle": "block",
        "foreground": "#aacfd1",
        "background": "#05080d",
        "cursor": "#aacfd1",
        "cursorAccent": "#aacfd1",
        "selection": "rgba(170,207,209,0.3)",
        "colorFilter": [
         "rotate(180)",
         "saturate(0.5)",
         "mix(0.7)"
        ]
    },
    "globe": {      
        "base": "#aacfd1",        
        "marker": "#df740c",        
        "pin": "#aacfd1",        
        "satellite": "#ff5c57"        
    },
    "injectCSS": "section#filesystem{left: 0;width:82vw} section#filesystem>h3.title, section#filesystem>div{width:82vw;padding-left:10px;} section#keyboard{display:none;}"
}
